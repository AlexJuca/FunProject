<resources xmlns:android="http://schemas.android.com/apk/res/android">

    
    
     <style name="Theme.Funproject" parent="@android:style/Theme.Holo.Light">
        <item name="android:actionBarItemBackground">@drawable/selectable_background_funproject</item>
        <item name="android:popupMenuStyle">@style/PopupMenu.Funproject</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Funproject</item>
        <item name="android:actionBarTabStyle">@style/ActionBarTabStyle.Funproject</item>
        <item name="android:actionDropDownStyle">@style/DropDownNav.Funproject</item>
        <item name="android:actionBarStyle">@style/ActionBar.Solid.Funproject</item>
        <item name="android:actionModeBackground">@drawable/cab_background_top_funproject</item>
        <item name="android:actionModeSplitBackground">@drawable/cab_background_bottom_funproject</item>
        <item name="android:actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Funproject</item>
       
    </style>

   
    <style name="ActionBar.Solid.Funproject" parent="@android:style/Widget.Holo.Light.ActionBar.Solid">
        <item name="android:background">@drawable/ab_solid_funproject</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_solid_funproject</item>
        <item name="android:backgroundSplit">@drawable/ab_bottom_solid_funproject</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Funproject</item>
        <item name="android:titleTextStyle">@style/ActionBar.Solid.Funproject.TitleTextStyle</item>
    </style>

    <style name="ActionBar.Solid.Funproject.TitleTextStyle" 
        parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        
        <item name="android:textColor">@color/actionbarTextColor</item>
    </style>
    <style name="ActionBar.Transparent.Funproject" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">@drawable/ab_transparent_funproject</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Funproject</item>
    </style>

    <style name="PopupMenu.Funproject" parent="@android:style/Widget.Holo.Light.ListPopupWindow">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_funproject</item>	
    </style>

    <style name="DropDownListView.Funproject" parent="@android:style/Widget.Holo.Light.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_funproject</item>
    </style>

    <style name="ActionBarTabStyle.Funproject" parent="@android:style/Widget.Holo.Light.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_funproject</item>
    </style>

    <style name="DropDownNav.Funproject" parent="@android:style/Widget.Holo.Light.Spinner">
        <item name="android:background">@drawable/spinner_background_ab_funproject</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_funproject</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_funproject</item>
    </style>
    
    <style name="ProgressBar.Funproject" parent="@android:style/Widget.Holo.Light.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_funproject</item>
    </style>
    
    <style name="ActionButton.CloseMode.Funproject" parent="@android:style/Widget.Holo.Light.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_funproject</item>
    </style>

    <!-- this style is only referenced in a Light.DarkActionBar based theme -->
    <style name="Theme.Funproject.Widget" parent="@android:style/Theme.Holo">
        <item name="android:popupMenuStyle">@style/PopupMenu.Funproject</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Funproject</item>
    </style>

</resources>
